<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.HomeController">
   <children>
      <BorderPane prefHeight="300.0" prefWidth="400.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <HBox prefHeight="191.0" prefWidth="400.0" BorderPane.alignment="CENTER">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label text="Music">
                           <font>
                              <Font size="25.0" />
                           </font>
                           <VBox.margin>
                              <Insets left="10.0" top="30.0" />
                           </VBox.margin>
                        </Label>
                        <Label text="SFX">
                           <font>
                              <Font size="25.0" />
                           </font>
                           <VBox.margin>
                              <Insets left="10.0" top="10.0" />
                           </VBox.margin>
                        </Label>
                     </children>
                  </VBox>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <ToggleButton mnemonicParsing="false" prefHeight="20.0" prefWidth="40.0">
                           <VBox.margin>
                              <Insets left="5.0" top="40.0" />
                           </VBox.margin>
                        </ToggleButton>
                        <ToggleButton lineSpacing="10.0" mnemonicParsing="false" prefWidth="40.0">
                           <VBox.margin>
                              <Insets left="5.0" top="17.0" />
                           </VBox.margin>
                        </ToggleButton>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </center>
         <bottom>
            <VBox prefHeight="93.0" prefWidth="400.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="btnSettingDone" alignment="CENTER" contentDisplay="CENTER" defaultButton="true" mnemonicParsing="false" onAction="#handleButtonAction" text="Done" VBox.vgrow="ALWAYS">
                     <font>
                        <Font name="Arial Black" size="25.0" />
                     </font>
                     <VBox.margin>
                        <Insets left="150.0" />
                     </VBox.margin>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
               </children>
            </VBox>
         </bottom>
         <top>
            <Button fx:id="btnChangeName" mnemonicParsing="false" onAction="#handleButtonAction" text="Change Name" BorderPane.alignment="CENTER">
               <font>
                  <Font size="20.0" />
               </font>
               <BorderPane.margin>
                  <Insets top="10.0" />
               </BorderPane.margin>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </top>
      </BorderPane>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</AnchorPane>
